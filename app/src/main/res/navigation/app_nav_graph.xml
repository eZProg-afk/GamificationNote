<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_nav_graph"
    app:startDestination="@id/dashboardFragment">

    <fragment
        android:id="@+id/dashboardFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/toAchievements"
            app:destination="@id/achievementsFragment" />
        <action
            android:id="@+id/toAchievementsConstructor"
            app:destination="@id/achievementsConstructorFragment" />
        <action
            android:id="@+id/toAppDecoration"
            app:destination="@id/appDecorationFragment" />
        <action
            android:id="@+id/toCreatorHome"
            app:destination="@id/creatorHomeFragment" />
        <action
            android:id="@+id/toRaids"
            app:destination="@id/raidsFragment" />
        <action
            android:id="@+id/toSettings"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/tasksFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.tasks.TasksFragment"
        android:label="fragment_tasks"
        tools:layout="@layout/fragment_tasks" />
    <fragment
        android:id="@+id/shopFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.shop.ShopFragment"
        android:label="fragment_shop"
        tools:layout="@layout/fragment_shop" />
    <fragment
        android:id="@+id/achievementsFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.achievements.AchievementsFragment"
        android:label="fragment_achievements"
        tools:layout="@layout/fragment_achievements" />
    <fragment
        android:id="@+id/achievementsConstructorFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.achievementsConstructor.AchievementsConstructorFragment"
