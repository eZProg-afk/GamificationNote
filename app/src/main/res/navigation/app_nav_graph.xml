            android:id="@+id/toHeroInventory"
            app:destination="@id/heroInventoryFragment" />
        <action
            android:id="@+id/toHeroCustomize"
            app:destination="@id/heroCustomizeFragment" />
        <action
            android:id="@+id/toHeroSkills"
            app:destination="@id/heroSkillsFragment" />
        <action
            android:id="@+id/toHeroStats"
            app:destination="@id/heroStatsFragment" />
    </fragment>
    <fragment
        android:id="@+id/heroInventoryFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.hero.inventory.HeroInventoryFragment"
        android:label="fragment_hero_inventory"
        tools:layout="@layout/fragment_hero_inventory" />
    <fragment
        android:id="@+id/heroCustomizeFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.hero.customize.HeroCustomizeFragment"
        android:label="fragment_hero_customize"
        tools:layout="@layout/fragment_hero_customize" />
    <fragment
        android:id="@+id/heroSkillsFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.hero.skills.HeroSkillsFragment"
        android:label="fragment_hero_skills"
        tools:layout="@layout/fragment_hero_skills" />
    <fragment
        android:id="@+id/heroStatsFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.hero.stats.HeroStatsFragment"
        android:label="fragment_hero_stats"
        tools:layout="@layout/fragment_hero_stats" />
    <fragment
        android:id="@+id/completedTasksFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.tasks.completed.CompletedTasksFragment"
        android:label="fragment_completed_tasks"
        tools:layout="@layout/fragment_completed_tasks" />
    <fragment
        android:id="@+id/compositeTasksFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.tasks.composite.CompositeTasksFragment"
        android:label="fragment_composite_tasks"
        tools:layout="@layout/fragment_composite_tasks" />
    <fragment
        android:id="@+id/foldersFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.tasks.folders.FoldersFragment"
        android:label="fragment_folders"
        tools:layout="@layout/fragment_folders" >
        <action
            android:id="@+id/toInFolderTasks"
            app:destination="@id/inFolderTasksFragment" />
    </fragment>
    <fragment
        android:id="@+id/inFolderTasksFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.tasks.folders.InFolderTasksFragment"
        android:label="fragment_in_folder_tasks"
        tools:layout="@layout/fragment_in_folder_tasks" />
    <fragment
        android:id="@+id/repeatingTasksFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.tasks.repeating.RepeatingTasksFragment"
        android:label="fragment_repeating_tasks"
        tools:layout="@layout/fragment_repeating_tasks" />
    <fragment
        android:id="@+id/simpleTasksFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.tasks.simple.SimpleTasksFragment"
        android:label="fragment_simple_tasks"
        tools:layout="@layout/fragment_simple_tasks" />
</navigation>