        tools:layout="@layout/fragment_app_decoration" />
    <fragment
        android:id="@+id/creatorHomeFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.creatorHome.CreatorHomeFragment"
        android:label="fragment_creator_home"
        tools:layout="@layout/fragment_creator_home" />
    <fragment
        android:id="@+id/guildBossFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.guild.boss.GuildBossFragment"
        android:label="fragment_guild_boss"
        tools:layout="@layout/fragment_guild_boss" />
    <fragment
        android:id="@+id/createGuildFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.guild.create.CreateGuildFragment"
        android:label="fragment_create_guild"
        tools:layout="@layout/fragment_create_guild" />
    <fragment
        android:id="@+id/guildChatFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.guild.chat.GuildChatFragment"
        android:label="fragment_guild_chat"
        tools:layout="@layout/fragment_guild_chat" />
    <fragment
        android:id="@+id/guildInfoFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.guild.info.GuildInfoFragment"
        android:label="fragment_guild_info"
        tools:layout="@layout/fragment_guild_info" />
    <fragment
        android:id="@+id/guildSearchFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.guild.search.GuildSearchFragment"
        android:label="fragment_guild_search"
        tools:layout="@layout/fragment_guild_search" />
    <fragment
        android:id="@+id/raidsFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.raids.RaidsFragment"
        android:label="fragment_raids"
        tools:layout="@layout/fragment_raids" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.settings.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/guildFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.guild.GuildFragment"
        android:label="fragment_guild"
        tools:layout="@layout/fragment_guild" >
        <action
            android:id="@+id/toGuildBoss"
            app:destination="@id/guildBossFragment" />
        <action
            android:id="@+id/toCreateGuild"
            app:destination="@id/createGuildFragment" />
        <action
            android:id="@+id/toGuildChat"
            app:destination="@id/guildChatFragment" />
        <action
            android:id="@+id/toGuildInfo"
            app:destination="@id/guildInfoFragment" />
        <action
            android:id="@+id/toGuildSearch"
            app:destination="@id/guildSearchFragment" />
    </fragment>
    <fragment
        android:id="@+id/heroFragment"
        android:name="spiral.bit.dev.gamificationnote.ui.features.dashboard.hero.HeroFragment"
